        <packagedElement xmi:type="uml:Class" xmi:id="_qLqiYJxJEemb3L5RebzWiw" name="ForecastApp">
          <ownedComment xmi:id="_qOfM4JxJEemb3L5RebzWiw">
            <body>A weather forecast app determines if the pressure readings from its
subscribed weather station, if any, indicate it is likely to rain due to a
reduction on the pressure level.</body>
          </ownedComment>
          <generalization xmi:id="_qNtw0JxJEemb3L5RebzWiw" general="_qMoywJxJEemb3L5RebzWiw"/>
          <ownedAttribute xmi:id="_qOEWIJxJEemb3L5RebzWiw" name="currPressure" visibility="private">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#double"/>
            <defaultValue xmi:type="uml:LiteralString" xmi:id="_qOE9MJxJEemb3L5RebzWiw" value="0.0"/>
          </ownedAttribute>
          <ownedAttribute xmi:id="_qOGyYJxJEemb3L5RebzWiw" name="lastPressure" isReadOnly="true">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#double"/>
            <defaultValue xmi:type="uml:LiteralString" xmi:id="_qOGyYZxJEemb3L5RebzWiw" value="0.0"/>
          </ownedAttribute>
          <ownedAttribute xmi:id="_qOHZcJxJEemb3L5RebzWiw" name="station" visibility="private" type="_qMr2EJxJEemb3L5RebzWiw" association="_qOkscJxJEemb3L5RebzWiw">
            <defaultValue xmi:type="uml:LiteralString" xmi:id="_qOHZcZxJEemb3L5RebzWiw" value="null"/>
          </ownedAttribute>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_qOX4IJxJEemb3L5RebzWiw" name="update" specification="_qLs-oZxJEemb3L5RebzWiw">
            <language>java</language>
            <body>if ((currPressure == 0.0) &amp;&amp; (lastPressure == 0.0)) {
			lastPressure = station.getPressure();
			currPressure = lastPressure;
		}
			
		lastPressure = currPressure;
		currPressure = station.getPressure();
	</body>
          </ownedBehavior>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_qOX4IZxJEemb3L5RebzWiw" name="getWeatherStation" specification="_qLtlsJxJEemb3L5RebzWiw">
            <ownedParameter xmi:id="_qOX4IpxJEemb3L5RebzWiw" type="_qMr2EJxJEemb3L5RebzWiw" direction="return"/>
            <language>java</language>
            <body>return station;
	</body>
          </ownedBehavior>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_qOYfMJxJEemb3L5RebzWiw" name="setWeatherStation" specification="_qNtJwZxJEemb3L5RebzWiw">
            <ownedParameter xmi:id="_qOYfMZxJEemb3L5RebzWiw" name="ws" type="_qMr2EJxJEemb3L5RebzWiw"/>
            <language>java</language>
            <body>station = ws;
	</body>
          </ownedBehavior>
