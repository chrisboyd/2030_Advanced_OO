        <packagedElement xmi:type="uml:Class" xmi:id="_qL6aAJxJEemb3L5RebzWiw" name="StatisticsApp">
          <ownedComment xmi:id="_qOfz85xJEemb3L5RebzWiw">
            <body>A weather statistics app calculates the minimum, maximum, and average
temperature read so far from its subscribed weather station, if any.</body>
          </ownedComment>
          <generalization xmi:id="_qNxbMZxJEemb3L5RebzWiw" general="_qMoywJxJEemb3L5RebzWiw"/>
          <ownedAttribute xmi:id="_qOKcwZxJEemb3L5RebzWiw" name="max" visibility="private">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#double"/>
            <defaultValue xmi:type="uml:LiteralString" xmi:id="_qOKcwpxJEemb3L5RebzWiw" value="Double.NaN"/>
          </ownedAttribute>
          <ownedAttribute xmi:id="_qOLD0ZxJEemb3L5RebzWiw" name="min" visibility="private">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#double"/>
            <defaultValue xmi:type="uml:LiteralString" xmi:id="_qOLD0pxJEemb3L5RebzWiw" value="Double.NaN"/>
          </ownedAttribute>
          <ownedAttribute xmi:id="_qOLq4ZxJEemb3L5RebzWiw" name="total" visibility="private">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#double"/>
            <defaultValue xmi:type="uml:LiteralString" xmi:id="_qOLq4pxJEemb3L5RebzWiw" value="0.0"/>
          </ownedAttribute>
          <ownedAttribute xmi:id="_qOMR8ZxJEemb3L5RebzWiw" name="readings" visibility="private">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
            <defaultValue xmi:type="uml:LiteralString" xmi:id="_qOMR8pxJEemb3L5RebzWiw" value="0"/>
          </ownedAttribute>
          <ownedAttribute xmi:id="_qOOHIJxJEemb3L5RebzWiw" name="station" visibility="private" type="_qMr2EJxJEemb3L5RebzWiw" association="_qOmhoZxJEemb3L5RebzWiw">
            <defaultValue xmi:type="uml:LiteralString" xmi:id="_qOOHIZxJEemb3L5RebzWiw" value="null"/>
          </ownedAttribute>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_qOaUZZxJEemb3L5RebzWiw" name="update" specification="_qL7oIJxJEemb3L5RebzWiw">
            <language>java</language>
            <body>double temp = station.getTemperature();
		readings++;
		total += temp;
		
		if (readings == 1) {
			max = temp;
			min = temp;
		}
		
		if (temp > max)
			max = temp;
		if (temp &lt; min)
			min = temp;
	</body>
          </ownedBehavior>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_qOaUZpxJEemb3L5RebzWiw" name="getWeatherStation" specification="_qL8PMJxJEemb3L5RebzWiw">
            <ownedParameter xmi:id="_qOaUZ5xJEemb3L5RebzWiw" type="_qMr2EJxJEemb3L5RebzWiw" direction="return"/>
            <language>java</language>
            <body>return station;
	</body>
          </ownedBehavior>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_qOa7cJxJEemb3L5RebzWiw" name="setWeatherStation" specification="_qNw0IZxJEemb3L5RebzWiw">
            <ownedParameter xmi:id="_qOa7cZxJEemb3L5RebzWiw" name="ws" type="_qMr2EJxJEemb3L5RebzWiw"/>
            <language>java</language>
            <body>station = ws;
	</body>
          </ownedBehavior>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_qOa7cpxJEemb3L5RebzWiw" name="getMinTemperature" specification="_qL82QJxJEemb3L5RebzWiw">
            <ownedParameter xmi:id="_qOa7c5xJEemb3L5RebzWiw" direction="return">
              <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#double"/>
            </ownedParameter>
            <language>java</language>
            <body>return min;
	</body>
          </ownedBehavior>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_qOa7dJxJEemb3L5RebzWiw" name="getMaxTemperature" specification="_qL9dUJxJEemb3L5RebzWiw">
            <ownedParameter xmi:id="_qOa7dZxJEemb3L5RebzWiw" direction="return">
              <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#double"/>
            </ownedParameter>
            <language>java</language>
            <body>return max;
	</body>
          </ownedBehavior>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_qOa7dpxJEemb3L5RebzWiw" name="getAverageTemperature" specification="_qL9dUpxJEemb3L5RebzWiw">
