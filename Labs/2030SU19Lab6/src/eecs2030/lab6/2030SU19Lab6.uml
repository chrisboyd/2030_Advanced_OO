            </ownedParameter>
            <language>java</language>
            <body>//this.update();
		
		return currPressure;
	</body>
          </ownedBehavior>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_qOZGQJxJEemb3L5RebzWiw" name="isLikelyToRain" specification="_qLuz0ZxJEemb3L5RebzWiw">
            <ownedParameter xmi:id="_qOZGQZxJEemb3L5RebzWiw" direction="return">
              <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
            </ownedParameter>
            <language>java</language>
            <body>//this.update();
		return currPressure &lt; lastPressure;
	</body>
          </ownedBehavior>
